<Section collapsed="false" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title>
<Text-field style="Heading 1" size="14" family="Calibri" layout="Heading 1"><Font family="Calibri" size="14">Interactive Optimal Portfolio Plot with Investor Preferences</Font></Text-field></Title><Presentation-Block>
<Group view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L1611" drawlabel="true">
<Input>
<Text-field style="Text" family="Calibri" layout="Normal"><Equation executable="false" style="2D Math" input-equation="" display="LUklbXJvd0c2Iy9JK21vZHVsZW5hbWVHNiJJLFR5cGVzZXR0aW5nR0koX3N5c2xpYkdGJzYlLUkjbWlHRiQ2I1EhRicvJSdmYW1pbHlHUShDYWxpYnJpRicvJSxtYXRodmFyaWFudEdRJ25vcm1hbEYn">JSFH</Equation><Font family="Calibri">The following plot uses the variables defined in the second section of this document to generate the efficient frontier and the optimal investor portfolio.</Font></Text-field>
</Input>
</Group></Presentation-Block><Presentation-Block>
<Group view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L2000" drawlabel="true">
<Input>
<Text-field style="Text" layout="Normal"></Text-field>
</Input>
</Group></Presentation-Block><Presentation-Block>
<Group view="presentation" hide-input="false" hide-output="true" inline-output="false" labelreference="L1613" drawlabel="true">
<Input>
<Text-field style="Text" family="Calibri" layout="Normal" alignment="centred"><EC-Button id="ButtonRefresh" caption="Refresh Plot" pixel-width="300" pixel-height="200" enabled="true" visible="true" show-border="true" user-size="false"><MapleCode action="click-event-handler"># Toggle Plot

use DocumentTools in 

# Generate Vector of Expected Returns (R)

Do(R = Vector([%Slidermu1, %Slidermu2, %Slidermu3], datatype=float)):

# Generate Matrix of Covariances (S)

Do(v = Vector([%Slidersig1, %Slidersig2, %Slidersig3]), datatype=float):
Do(k = %Sliderk):

S := Matrix(3):

for i from 1 to 3 do
for j from 1 to 3 do
S[i,j] := k*v[i]*v[j]
end do;
end do;

for i from 1 to 3 do
S[i,i] := S[i,i]/k
end do:


# Generate Equation for sigma

u3 := Vector(1 .. 3, 1):

u3t := u3 ^ +:
Rt := R ^ +:
Q := linalg[inverse](S):

a := u3t . Q . u3:
b := u3t . Q . R:
c := Rt . Q . R:
d := a*c-b^2:

sigmaP := sqrt((a*(mu^2)-2*b*mu+c)/d):

# Plot Markowitz Frontier

with(plots):

rBound := 1.5*max(v[1],v[2],v[3]):
vBound := 2*max(R[1],R[2],R[3]);

P1 := plot([sigmaP, mu, mu=-1..vBound],Risk =0..rBound, Reward =0..vBound, labels=[&quot;Risk&quot;, &quot;Reward&quot;]):

P2 := plot([[v[1], R[1]], [v[2], R[2]], [v[3], R[3]]], style = point, color = blue, symbol = asterisk, symbolsize = 10):

# Plot CAL

with(LinearAlgebra):
Do(r = %Sliderr): 
w := (Q.(R-r*u3))/(b-r*a):
sigmam := sqrt(w^+.S.w):
mum := w^+.R: 
slope := (mum-r)/sigmam:
cal := slope*sigma+r:

P3 := plot(cal, sigma = 0 .. rBound, color = &quot;green&quot;): 

P4 := plot([[sigmam, mum]], style = point, color=&quot;Violet&quot;, symbol = box, symbolsize = 20, legend=[&quot;Market Portfolio&quot;]):

# Maximize utility and plot

Do(B = min(1/r,%SliderB)):

utilStar := solve({mu = 1/B-sigma/slope, mu = slope*sigma+r}, [sigma, mu])[1]:

optimUtil := subs(utilStar[1], utilStar[2], mu-(1/2)*B*(mu^2+sigma^2)):

utilCurve := sqrt(2*(mu-optimUtil)/B-mu^2):
expUtilStar := subs(utilStar, [sigma, mu]):

P5 := plot([utilCurve,mu, mu=-2.. vBound], color=&quot;Orange&quot;):

P6 := plot([expUtilStar], style = point, color=&quot;Orange&quot;, symbol = diamond, symbolsize = 20, legend=[&quot;Investor Portfolio&quot;]):

Do(%PlotFinal = display([P1, P2, P3, P4, P5, P6], title=&quot;Interactive Markowitz Efficient Frontier&quot;, titlefont = [TIMES, 18])):

end use; 
</MapleCode></EC-Button></Text-field>
</Input>
