DocumentTools[Do](v = Vector([%Slidersig1, %Slidersig2, %Slidersig3]), datatype=float):
DocumentTools[Do](k = %Sliderk):

S := Matrix(3):

for i from 1 to 3 do
for j from 1 to 3 do
S[i,j] := k*v[i]*v[j]
end do;
end do;

for i from 1 to 3 do
S[i,i] := S[i,i]/k
end do:
